{
	"name": "@bubbles/shared",
	"version": "1.0.0",
	"description": "Shared types and utilities for Discord bot",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"clean": "rm -rf dist",
		"setup": "npm install && npm run db:generate && npm run build"
	},
	"dependencies": {
		"@prisma/client": "^6.9.0",
		"bull": "^4.12.9"
	},
	"devDependencies": {
		"@types/bull": "^4.10.0",
		"prisma": "^6.9.0",
		"typescript": "^5.3.3"
	},
	"exports": {
		".": "./dist/index.js",
		"./database": "./dist/database.js",
		"./types/*": "./dist/types/*.js",
		"./utils/*": "./dist/utils/*.js"
	}
}
