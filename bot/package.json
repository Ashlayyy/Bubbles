{
  "name": "ashlay-bot",
  "version": "1.0.0",
  "description": "A discord bot.",
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "main": "build/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "postbuild": "node build/src/scripts/generateConfig.js",
    "dev": "nodemon",
    "dev:shard": "tsx shard.ts",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "commands:register": "node build/src/scripts/discordAPI/registerCommands.js",
    "commands:resetDev": "tsc && cross-env NODE_ENV=development tsx src/scripts/discordAPI/resetCommands.ts",
    "commands:resetProd": "node build/src/scripts/discordAPI/resetCommands.js",
    "start": "node .",
    "start:shard": "node build/shard.js",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@bubbles/shared": "file:../shared",
    "@twemoji/parser": "^16.0.0",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "bull": "^4.12.9",
    "discord-emoji-converter": "^1.2.10",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.7",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mediaplex": "^1.0.0",
    "rimraf": "^6.0.1",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/bull": "^4.10.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.10",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/parser": "^8.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^3.8.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "engines": {
    "node": ">=20.10.0"
  }
}
